{
        "service": {
                "dns": {
                        "forwarding": {
                                "options": [
                                        "domain-needed",
                                        "bogus-priv",
                                        "stop-dns-rebind",
                                        "server=127.0.0.1#6878",
                                        "trust-anchor=.,20326,8,2,E06D44B80B8F1D39A95C0B0D7C65D08458E880409BBC683457104237C7F8EC8D",
                                        "dnssec",
                                        "dnssec-no-timecheck",
										"no-resolv"
                                ]
                        }
                },
		        "upnp2": {
                        "acl": {
                                "rule": {
                                        "10": {
                                                "action": "deny",
                                                "description": "deny access to external Port 3074 (Xbox Live)",
                                                "external-port": "3074",
                                                "local-port": "0-65535",
                                                "subnet": "192.168.0.0/24"
                                        }
                                }
                        },
                        "listen-on": [
                                "eth1"
                        ],
                        "nat-pmp": "enable",
                        "secure-mode": "enable",
                        "wan": "eth0"
                }
        }
}