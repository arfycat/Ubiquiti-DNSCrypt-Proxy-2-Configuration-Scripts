{
        "service": {
                "dns": {
                        "forwarding": {
                                "options": [
                                        "bogus-priv",
                                        "stop-dns-rebind",
										"no-resolv",
                                        "server=127.0.0.1#6878"
                                ]
                        }
                },
		        "upnp2": {
                        "acl": {
                                "rule": {
                                        "10": {
                                                "action": "deny",
                                                "description": "deny access to external Port 3074 (Xbox Live)",
                                                "external-port": "3074",
                                                "local-port": "0-65535",
                                                "subnet": "192.168.0.0/24"
                                        }
                                }
                        },
                        "listen-on": [
                                "eth1"
                        ],
                        "nat-pmp": "enable",
                        "secure-mode": "enable",
                        "wan": "eth0"
                }
        }
}